{
  "name": "box2dweb",
  "version": "1.0.0",
  "description": "a port of box2dweb (itself a port of box2dflash [itself a port of box2d]) to typescript and es modules",
  "repository": "git@github.com:mysterycommand/box2dweb.git",
  "author": "Matt Hayes <matt@mysterycommand.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf .cache/ coverage/ dist/ node_modules/",
    "build": "parcel build src/demo/box2dweb.html src/demo/html5canvas/testbed.html --public-url /box2dweb --experimental-scope-hoisting",
    "start": "parcel src/demo/box2dweb.html src/demo/html5canvas/testbed.html",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest",
    "coverage": "open coverage/lcov-report/index.html"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1",
    "yarn": "1.12.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-typescript": "7.1.0",
    "@semantic-release/changelog": "3.0.1",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/git": "7.0.5",
    "@semantic-release/github": "5.2.1",
    "@semantic-release/npm": "5.0.5",
    "@semantic-release/release-notes-generator": "7.1.3",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.2",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "parcel-bundler": "1.10.3",
    "prettier": "1.14.3",
    "semantic-release": "15.10.7",
    "semantic-release-github-pr": "5.1.0",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.1.6"
  }
}
